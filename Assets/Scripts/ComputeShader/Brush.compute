#include "Utils.hlsl"
#pragma kernel CSMain
#pragma kernel Clear

RWTexture2D<float4> Result;
int seed;

uint size;
uint kind;
uint2 pos;

[numthreads(8,8,1)]
void CSMain (uint3 id : SV_DispatchThreadID)
{
  if(distance(id.xy,pos)<size)
        Result[id.xy]=int2float(kind);
}

[numthreads(8,8,1)]
void Clear(uint3 id : SV_DispatchThreadID)
{
    Result[id.xy]=0;
}